[Service]
# k3s-agent service override configuration
# Generated by Ansible

# Restart configuration
Restart=always
RestartSec=5s

# Resource limits
LimitNOFILE=1048576
LimitNPROC=1048576
LimitCORE=infinity

# Additional environment variables
{% if k3s_agent_extra_args %}
Environment="K3S_EXEC_ARGS={{ k3s_agent_extra_args | join(' ') }}"
{% endif %}

# Logging
StandardOutput=journal
StandardError=journal